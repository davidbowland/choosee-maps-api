{
  "name": "choosee-maps-api",
  "version": "1.19.0",
  "description": "Lambda choosee maps API",
  "author": "David Bowland",
  "engines": {
    "node": "^20.0.0"
  },
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/davidbowland/choosee-maps-api.git"
  },
  "bugs": {
    "url": "https://github.com/davidbowland/choosee-maps-api/issues"
  },
  "homepage": "https://github.com/davidbowland/choosee-maps-api#readme",
  "scripts": {
    "build": "npm run clean && tsc && cp package*.json dist/ && cd dist/ && NODE_ENV=production npm ci",
    "clean": "rm -rf .aws-sam dist coverage && NODE_ENV=test npm ci",
    "deploy": "npm run build && ./scripts/deploy.sh",
    "lint": "prettier --write . && eslint --fix .",
    "prepare": "husky install",
    "start": "npm run build && npm run quick-start",
    "quick-start": "tsc && ./scripts/runLocal.sh",
    "test": "jest --colors",
    "typecheck": "tsc --noEmit",
    "update": "ncu --doctor --target minor --upgrade && npm audit fix --audit-level=none && npm run test && npm dedupe"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.441.0",
    "@googlemaps/google-maps-services-js": "^3.3.41",
    "aws-xray-sdk-core": "^3.5.3",
    "axios": "^1.6.0",
    "axios-retry": "^3.8.1"
  },
  "devDependencies": {
    "@babel/core": "^7.23.2",
    "@babel/preset-env": "^7.23.2",
    "@babel/preset-typescript": "^7.23.2",
    "@types/aws-lambda": "^8.10.125",
    "@types/jest": "^29.5.7",
    "@types/node": "^18.18.8",
    "@typescript-eslint/eslint-plugin": "^6.9.1",
    "@typescript-eslint/parser": "^6.9.1",
    "babel-jest": "^29.7.0",
    "eslint": "^8.53.0",
    "eslint-config-prettier": "^9.0.0",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "lint-staged": "^15.0.2",
    "msw": "^1.3.2",
    "prettier": "^3.0.3",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx}": [
      "prettier --write --ignore-unknown",
      "eslint --fix",
      "jest --colors --findRelatedTests --coverageThreshold '{}' --passWithNoTests"
    ],
    "**/*": [
      "prettier --write --ignore-unknown"
    ]
  }
}
